{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Frontend: Next.js Dev",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/app/frontend",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["run", "dev", "--", "--inspect"],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
